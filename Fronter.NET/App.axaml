             xmlns:fronter="clr-namespace:Fronter"
             x:Class="Fronter.App"
             RequestedThemeVariant="Default">
    <Application.DataTemplates>
        <fronter:ViewLocator/>
    </Application.DataTemplates>
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"></StyleInclude>
    </Application.Styles>
</Application>
